<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="15" alignment="CENTER"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.blackjack.view.MainMenuController">

    <Label text="BLACKJACK" fx:id="gameLabel"/>
    <Button text="Start" onAction="#onStart"/>
    <Button text="Settings" onAction="#onSettings"/>
    <Button text="High scores" onAction="#onHighscores"/>
    <Button text="Exit" onAction="#onExit"/>

</VBox>
